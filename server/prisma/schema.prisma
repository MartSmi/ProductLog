datasource db {
  provider = "sqlite"
  url      = "file:./data.db?connection_limit=1"
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id        String @id @default(cuid())
  name      String
  EAN       String? @unique
  artNumber String
  quantity  Int @default(0)
  store     String
  @@unique([artNumber, store])
}